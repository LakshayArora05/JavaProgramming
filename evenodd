import java.util.Scanner;
 class evenoddcheck {
  public static void main(String[] args) {
   Scanner read = new Scanner(System.in);
   System.out.println("Enter any Number:");
   int a;
   a=read.nextInt();
   if (a%2==0)
   System.out.println("even");
   else
   System.out.println("odd");
   }
   }
   
